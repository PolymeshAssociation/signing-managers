{
  "extends": "@nx/workspace/presets/core.json",
  "npmScope": "signing-managers",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nx/workspace"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nx/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "release": [
      {
        "target": "build",
        "projects": "self"
      },
      {
        "target": "release",
        "projects": "dependencies"
      }
    ]
  }
}
